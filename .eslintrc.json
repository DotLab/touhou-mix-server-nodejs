{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
},
  "rules": {
    "no-multiple-empty-lines": ["error", {"max": 1}],
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "max-len": "off"
  }
}
